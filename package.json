{
	"name": "vscode-hyphaene",
	"publisher": "hyphaene",
	"displayName": "home",
	"description": "custom view for personal purposes",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/hyphaene/vscode-extension"
	},
	"bugs": {
		"url": "https://github.com/hyphaene/vscode-extension/issues",
		"email": "maximilien.garenne@gmail.com"
	},
	"homepage": "https://github.com/hyphaene/vscode-extension/blob/master/README.md",
	"version": "0.0.23",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Snippets",
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:home.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "home.helloWorld",
				"title": "Hello World"
			}
		],
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/dist/javascript.json"
			},
			{
				"language": "typescript",
				"path": "./snippets/dist/typescript.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/dist/javascriptreact.json"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets/dist/typescriptreact.json"
			}
		],
		"keybindings": [
			{
				"key": "ctrl+alt+n",
				"command": "workbench.action.terminal.new"
			},
			{
				"key": "ctrl+alt+w",
				"command": "workbench.action.terminal.kill"
			},
			{
				"key": "ctrl+v",
				"command": "workbench.action.terminal.paste",
				"when": "terminalFocus"
			},
			{
				"key": "ctrl+c",
				"command": "workbench.action.terminal.copySelection",
				"when": "terminalFocus && terminalTextSelected"
			},
			{
				"key": "ctrl+numpad2",
				"command": "workbench.action.splitEditor"
			},
			{
				"key": "ctrl+numpad_add",
				"command": "workbench.action.maximizeEditor"
			},
			{
				"key": "alt+left",
				"command": "workbench.action.moveEditorToPreviousGroup"
			},
			{
				"key": "alt+right",
				"command": "workbench.action.moveEditorToNextGroup"
			},
			{
				"key": "ctrl+alt+u",
				"command": "workbench.action.toggleSidebarVisibility"
			},
			{
				"key": "shift+enter",
				"command": "extension.jumpy-word",
				"when": "editorTextFocus"
			},
			{
				"key": "Escape",
				"command": "extension.jumpy-exit",
				"when": "editorTextFocus && jumpy.isJumpyMode"
			},
			{
				"key": "ctrl+i",
				"command": "editor.action.replaceOne",
				"when": "editorFocus && findWidgetVisible"
			},
			{
				"key": "ctrl+alt+i",
				"command": "editor.action.replaceAll",
				"when": "editorFocus && findWidgetVisible"
			},
			{
				"key": "ctrl+alt+pageup",
				"command": "workbench.action.toggleMaximizedPanel"
			},
			{
				"key": "ctrl+k ctrl+left",
				"command": "cursorLineStart"
			},
			{
				"key": "ctrl+k ctrl+right",
				"command": "cursorLineEnd"
			},
			{
				"key": "ctrl+p",
				"command": "workbench.action.quickOpen"
			},
			{
				"key": "ctrl+shift+l",
				"command": "editor.action.deleteLines",
				"when": "editorTextFocus"
			},
			{
				"key": "ctrl+alt+q",
				"command": "workbench.action.quit"
			},
			{
				"key": "ctrl+v",
				"command": "editor.action.clipboardPasteAction",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"key": "ctrl+d",
				"command": "editor.action.addSelectionToNextFindMatch",
				"when": "editorFocus"
			},
			{
				"key": "ctrl+s",
				"command": "workbench.action.files.save"
			},
			{
				"key": "ctrl+alt+s",
				"command": "workbench.action.files.saveAll"
			},
			{
				"key": "ctrl+z",
				"command": "undo",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"key": "ctrl+shift+f",
				"command": "workbench.view.search",
				"when": "!searchViewletVisible"
			},
			{
				"key": "ctrl+l",
				"command": "expandLineSelection"
			},
			{
				"key": "ctrl+a",
				"command": "workbench.action.terminal.selectAll",
				"when": "terminalFocus"
			},
			{
				"key": "ctrl+a",
				"command": "editor.action.selectAll"
			},
			{
				"key": "ctrl+c",
				"command": "editor.action.clipboardCopyAction",
				"when": "textInputFocus"
			},
			{
				"key": "ctrl+y",
				"command": "redo"
			},
			{
				"key": "ctrl+shift+h",
				"command": "workbench.action.replaceInFiles"
			},
			{
				"key": "ctrl+f",
				"command": "settings.action.search",
				"when": "inSettingsEditor"
			},
			{
				"key": "ctrl+alt+k",
				"command": "workbench.action.openGlobalKeybindings"
			},
			{
				"key": "ctrl+k ctrl+s",
				"command": "workbench.action.openGlobalKeybindingsFile"
			},
			{
				"key": "ctrl+alt+g",
				"command": "workbench.view.scm"
			},
			{
				"key": "ctrl+alt+o",
				"command": "workbench.action.terminal.toggleTerminal"
			},
			{
				"key": "ctrl+/",
				"command": "editor.action.commentLine",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"key": "ctrl+alt+`",
				"command": "workbench.files.action.focusFilesExplorer"
			},
			{
				"key": "ctrl+alt+;",
				"command": "workbench.action.focusActiveEditorGroup"
			},
			{
				"key": "ctrl+alt+[",
				"command": "workbench.files.action.focusOpenEditorsView",
				"when": "workbench.explorer.openEditorsView.active"
			},
			{
				"key": "ctrl+alt+oem_8",
				"command": "npm.focus"
			},
			{
				"key": "ctrl+alt+m",
				"command": "workbench.panel.terminal.focus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"commit": "git-cz",
		"prerelease": "npm run snippets:create;git add --all; npm run compile",
		"release": "standard-version",
		"postrelease": "git push --follow-tags origin master",
		"package": "vsce package",
		"submit": "npm run release && vsce publish && npm publish && git push",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"snippets:create": "ts-node src/scripts/createSnippets.ts"
	},
	"devDependencies": {
		"@commitlint/cli": "^8.2.0",
		"@commitlint/config-conventional": "^8.2.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.1",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"cz-conventional-changelog": "^3.0.2",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"husky": "^3.1.0",
		"mocha": "^7.1.1",
		"nodemon": "^2.0.3",
		"standard-version": "^7.0.1",
		"ts-node": "^8.8.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
